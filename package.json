{
  "name": "cel-typescript",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "types": "./dist/src/index.d.ts",
  "napi": {
    "name": "cel-typescript",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-pc-windows-msvc",
        "armv7-linux-androideabi",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-gnu",
        "x86_64-apple-darwin",
        "x86_64-pc-windows-msvc"
      ]
    }
  },
  "scripts": {
    "build:native": "nx build:native",
    "build:ts": "nx build:ts",
    "format": "nx run cel-typescript:format",
    "format:check": "nx run cel-typescript:format:check",
    "lint": "nx lint",
    "check": "nx check",
    "build": "nx build",
    "test": "nx test",
    "clean": "nx clean"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@napi-rs/cli": "^2.18.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.0.0",
    "jest": "^29.5.0",
    "mocha": "^11.1.0",
    "nx": "^20.8.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "vitest": "^3.1.2"
  }
}
